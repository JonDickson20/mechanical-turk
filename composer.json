{
    "name": "oldtimeguitarguy/mechanical-turk",
    "description": "An easily-extensible php client for the Amazon Mechanical Turk API",
    "require": {
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Karl Hepler",
            "email": "karl.hepler@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "OldTimeGuitarGuy\\MechanicalTurk\\": "src/OldTimeGuitarGuy/MechanicalTurk/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/helpers/OperationTestCase.php"
        ],
        "files": [
            "tests/helpers/helpers.php"
        ]
    }
}
